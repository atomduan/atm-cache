#!/bin/bash
cd $ATM_PROJECT_HOME

LC_ALL=C
export LC_ALL
# Config : var(cfg var use for other file and Makefile) check 
# and setting and header gen...
# sources var setting
. auto/sources
# system header info config
. auto/headers
# OS specific features config
. auto/os/conf
# third party lib features checking and config
. auto/lib/conf

# Makefile Gen...
. auto/make
. auto/lib/make
. auto/install

# Summary Gen...
. auto/summary
