#!/bin/sh 
LC_ALL=C
export LC_ALL
. auto/options
. auto/init

# Config : var check and setting and header gen...
# sources var setting
. auto/sources
# compiler var setting
. auto/cc/conf
# system header info config
. auto/headers
# OS specific features config
. auto/os/conf
# unix features senity checking and config
. auto/unix
# third party lib features checking and config
. auto/lib/conf

# Makefile Gen...
. auto/make
. auto/lib/make
. auto/install

# Summary Gen...
. auto/summary
